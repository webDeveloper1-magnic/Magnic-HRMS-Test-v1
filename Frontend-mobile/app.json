{
  "expo": {
    "name": "HRMS Mobile",
    "slug": "hrms-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "hrms-mobile",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saravana.hrmsmobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.saravana.hrmsmobile",
      "permissions": [
        "android.permission.INTERNET"
      ],
      "blockedPermissions": [
        "android.permission.DETECT_SCREEN_CAPTURE"
      ]
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "15a65c7a-daa6-4a07-8e4a-37e5a18ceb3e"
      }
    },
    "owner": "saravana-magnic",
    "plugins": [
      "expo-font"
    ]
  }
}
